#!/usr/bin/env node

var exec = require('child_process').exec,
    path = require('path');

exec(require('phantomjs').path + ' ' + path.join(__dirname, '../lib/https-scan.js ' + process.argv[2]), function(err, stdout, stderr) {
    console.log(err);
    console.log(stdout);
    console.log(stderr);
});
